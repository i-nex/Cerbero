' Gambas class file

Public rCPUDB As Result
Public Sub _new()
  
    
  If Request.Method = "POST" Then
    If Request.Post["cpudb"] = "edit" Then
    rCPUDB = DB.Edit("jqi5")
    Else If Request.Post["cpudb"] = "add" Then
    rCPUDB = DB.Create("jqi5")
    Endif
    rCPUDB!PROCESSOR_MODEL_NAME = Request.Post["PROCESSOR_MODEL_NAME"]
    rCPUDB!PROCESSOR_CODENAME = Request.Post["PROCESSOR_CODENAME"]
    rCPUDB!PROCESSOR_RELEASED = Request.Post["PROCESSOR_RELEASED"]
    rCPUDB!PROCESSOR_PRODUCTION_STATUS = Request.Post["PROCESSOR_PRODUCTION_STATUS"]
    rCPUDB!PROCESSOR_PART = Request.Post["PROCESSOR_PART"]
    rCPUDB!PROCESSOR_MARKET = Request.Post["PROCESSOR_MARKET"]
    rCPUDB!PROCESSOR_TDP = Request.Post["PROCESSOR_TDP"]
    rCPUDB!PROCESSOR_VOLTAGE = Request.Post["PROCESSOR_VOLTAGE"]
    rCPUDB!PROCESSOR_MULTIPLIER = Request.Post["PROCESSOR_MULTIPLIER"]
    rCPUDB!PROCESSOR_TURBOCLOCK = Request.Post["PROCESSOR_TURBOCLOCK"]
    rCPUDB!PROCESSOR_MEMORYSUPPORT = Request.Post["PROCESSOR_MEMORYSUPPORT"]
    rCPUDB!PROCESSOR_OFCORES = Request.Post["PROCESSOR_OFCORES"]
    rCPUDB!PROCESSOR_SMPCPUS = Request.Post["PROCESSOR_SMPCPUS"]
    rCPUDB!PROCESSOR_INTEGRATEDGRAPHIC = Request.Post["PROCESSOR_INTEGRATEDGRAPHIC"]
    rCPUDB!PROCESSOR_CACHEL1 = Request.Post["PROCESSOR_CACHEL1"]
    rCPUDB!PROCESSOR_CACHEL2 = Request.Post["PROCESSOR_CACHEL2"]
    rCPUDB!PROCESSOR_CACHEL3 = Request.Post["PROCESSOR_CACHEL3"]
    rCPUDB!PROCESSOR_FREQUENCY = Request.Post["PROCESSOR_FREQUENCY"]
    rCPUDB!PROCESSOR_TCASEMAX = Request.Post["PROCESSOR_TCASEMAX"]
    rCPUDB!PROCESSOR_PACKAGE = Request.Post["PROCESSOR_PACKAGE"]
    rCPUDB!PROCESSOR_DIE_SIZE = Request.Post["PROCESSOR_DIE_SIZE"]
    rCPUDB!PROCESSOR_TRANSISTORS = Request.Post["PROCESSOR_TRANSISTORS"]
    rCPUDB!PROCESSOR_PROCESS_SIZE = Request.Post["PROCESSOR_PROCESS_SIZE"]
    rCPUDB!PROCESSOR_SOCKET = Request.Post["PROCESSOR_SOCKET"]
    rCPUDB!PROCESSOR_SOCKET_IMAGE = Request.Post["PROCESSOR_SOCKET_IMAGE"]
    rCPUDB.Update
  Endif
  
End