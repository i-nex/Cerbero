' Gambas class file

Public rCPUDB As Result
Public Sub _new()
  Dim cpuname As String = Request.Get["cpuname"]
  
  If Request.Method = "GET" Then
    rCPUDB = DB.Edit("jqi5", "PROCESSOR_MODEL_NAME = &1", cpuname)
  Endif

End

